{
  "name": "slickflow-designer",
  "version": "3.5.0",
  "description": "A .net workflow engine for BPMN",
  "main": "app/index.js",
  "scripts": {
    "all": "run-s build",
    "build": "cross-env KCONFIG_ENV=dev webpack",
    "build:prod": "cross-env KCONFIG_ENV=prod webpack",
    "build:dev": "cross-env KCONFIG_ENV=dev webpack",
    "build:watch": "cross-env KCONFIG_ENV=dev webpack --watch",
    "start": "run-s build serve",
    "dev": "run-p build:watch serve",
    "serve": "sirv public --dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/besley/slickflow",
    "directory": "source"
  },
  "keywords": [
    "bpmnjs",
    "bpmn2"
  ],
  "author": {
    "name": "Besley",
    "url": "https://github.com/besley"
  },
  "contributors": [
    {
      "name": "slickflow contributors",
      "url": "https://github.com/besley"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-react-jsx": "^7.16.5",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.1.0",
    "css-loader": "^6.5.1",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "cross-env": "^7.0.3",
    "raw-loader": "^4.0.2",
    "sirv-cli": "^1.0.14",
    "style-loader": "^3.3.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@bpmn-io/properties-panel": "^2.0.0",
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@popperjs/core": "^2.11.5",
    "ag-grid-community": "^33.0.2",
    "bootstrap": "^4.5.0",
    "bootstrap-icons": "^1.8.1",
    "bootstrap5-dialog": "^5.0.1",
    "bpmn-auto-layout": "^1.0.0",
    "bpmn-js": "^18.1.1",
    "bpmn-js-properties-panel": "^2.0.0",
    "codemirror": "^5.65.3",
    "cron-expression-input": "^1.2.7",
    "fine-uploader": "^5.16.2",
    "jquery": "^3.5.1",
    "min-dash": "^3.7.0",
    "uuid": "^8.3.2"
  }
}
